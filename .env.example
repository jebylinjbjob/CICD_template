# PortalApi Environment Configuration
# Copy this file to .env and update the values

# Database Configuration
POSTGRES_DB=PortalApiDb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Application Configuration
ASPNETCORE_ENVIRONMENT=Production

# Connection Strings (use double underscores for nested config)
ConnectionStrings__Default=Host=postgres;Port=5432;Database=PortalApiDb;Username=postgres;Password=postgres

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning
